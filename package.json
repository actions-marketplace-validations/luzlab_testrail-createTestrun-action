{
  "name": "create-tesrail-testrun",
  "version": "1.0.0",
  "description": "This action runs when a PR is opened and creates a linked TestRun in TestRail",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf build dist && mkdirp dist build",
    "build": "run-s clean build-all",
    "build-all": "run-p build:*",
    "build:action": "ncc build src/index.ts ",
    "build:uiscript": "rollup -c rollup.config.js && node bundleUIScript.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "@octokit/webhooks-types": "^4.2.1",
    "deepmerge": "^4.2.2",
    "got": "^11.8.2",
    "rimraf": "^3.0.2",
    "testrail-api": "^1.3.6",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^1.10.0",
    "@rollup/plugin-commonjs": "^19.0.2",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^8.2.3",
    "@types/jquery": "^3.5.6",
    "@types/node": "^16.4.3",
    "@vercel/ncc": "^0.29.0",
    "js-base64": "^3.6.1",
    "ky": "^0.28.5",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "octokit": "^1.1.0",
    "rollup": "^2.54.0",
    "typescript": "^4.3.5"
  }
}
